{
  "name": "jonathan-timezone-robot",
  "version": "1.1.0",
  "description": "Translates times in slack with a message shortcut",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js --trace-warnings",
    "clean": "rm -rf ./build",
    "babel-build": "babel src --out-dir build --source-maps",
    "build": "npm run clean; npm run babel-build",
    "start": "node build/index.js"
  },
  "author": "lukec11 <me@lukec.me>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "@slack/bolt": "^2.4.1",
    "chrono-node": "^2.1.8",
    "dotenv": "^8.2.0",
    "honeybadger": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.19.3",
    "eslint": "^7.11.0",
    "nodemon": "^2.0.6"
  }
}
